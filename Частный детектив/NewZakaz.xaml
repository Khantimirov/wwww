<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Частный_детектив"
        xmlns:SqlClient="clr-namespace:System.Data.SqlClient;assembly=System.Data" x:Class="Частный_детектив.NewZakaz"
        mc:Ignorable="d"
        Title="Новый заказ" Height="400" Width="400" Loaded="Window_Loaded" Closed="Window_Closed" MinWidth="400" MinHeight="400" Icon="log.png">
    <DockPanel LastChildFill="False">
        <Image DockPanel.Dock="Top" Source="/logo.png" />
        <TextBlock Text="Услуга" DockPanel.Dock="Top"/>
        <ComboBox x:Name="listBox1" DockPanel.Dock="Top" Margin="20 0 20 0" FontSize="16"/>
        <TextBlock Text="Пожелания" DockPanel.Dock="Top"/>
        <TextBox x:Name="textBoxTextUslugi" DockPanel.Dock="Top" />
        <Button Content="Создать заказ" DockPanel.Dock="Top" Click="Button_Click"/>

    </DockPanel>
</Window>

